{
  "name": "random-forest-visualization",
  "scripts": {
    "gui": "npm run build:js && npm run server",
    "cli": "node --experimental-modules cli.mjs data",
    "server": "cd dist && node index.js data",
    "build:js": "rollup -c rollup.config.mjs",
    "watch:js": "rollup -c rollup.config.mjs --watch"
  },
  "devDependencies": {
    "rollup": "^0.59.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-scss": "^0.4.0"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "d3": "^5.4.0",
    "d3-component": "^3.1.0",
    "d3-node": "^1.1.3",
    "express": "^5.0.0-alpha.6",
    "yargs": "^11.0.0"
  }
}
