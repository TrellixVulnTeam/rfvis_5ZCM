    "parallelshell": "^3.0.2",
    "rollup": "^0.59.4"
  },
  "dependencies": {
    "canvas": "^1.6.11",
    "d3-node": "^1.1.3",
    "express": "^4.16.3",
    "svg2png": "^4.1.1"
  }
}
