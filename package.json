{
  "name": "random-forest-visualization",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run build:backend && npm run build:frontend && npm run build:assets",
    "build:backend": "rollup -c rollup.backend.js",
    "build:frontend": "rollup -c rollup.frontend.js",
    "build:assets": "cp index.html dist/index.html",
    "watch:backend": "rollup -c rollup.backend.js --watch",
    "watch:frontend": "rollup -c rollup.frontend.js --watch"
  },
  "devDependencies": {
    "rollup": "^0.59.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-scss": "^0.4.0"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "d3": "^5.4.0",
    "d3-component": "^3.1.0",
    "d3-node": "^1.1.3",
    "express": "^5.0.0-alpha.6",
    "yargs": "^11.0.0"
  }
}
